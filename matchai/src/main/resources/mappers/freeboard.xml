<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.matchai.board.mapper.FreeBoardMapper">

<select id="getFreeBoardList" resultType="hashmap">
        SELECT brdno, title, username, hitcount, del_flag
        FROM baseball.tbl_freeboard
        ORDER BY brdno
</select>

</mapper>
