<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.matchai.board.mapper.GameMapper">

    <select id="getGameList" resultType="hashmap">
        select tbn.brdno,
               tbn.brd_code ,
               tbn.match_code as matchcode,
               tbn.title,
               tbn.memo,
               tbn.brd_date,
               tbn.brd_user
        from tbl_board_nt01 tbn
    </select>

</mapper>